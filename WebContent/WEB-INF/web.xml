  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Contact</servlet-name>
    <url-pattern>/contact</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AccueilUniv</servlet-name>
    <url-pattern>/accueil/universite</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AccueilDD</servlet-name>
    <url-pattern>/accueil/doubleDiplome</url-pattern>
  </servlet-mapping>
</web-app>